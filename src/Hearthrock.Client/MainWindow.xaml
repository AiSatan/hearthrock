<Window x:Class="Hearthrock.Client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hearthrock.Client"
        mc:Ignorable="d"
        Title="Hearthrock" Height="300" Width="400">
    <Grid>
        <Button x:Name="PatchButton" Content="Patch" HorizontalAlignment="Left" Margin="80,40,0,0" VerticalAlignment="Top" Width="100" Height="30" Click="PatchButton_Click"/>
        <Button x:Name="UnpatchButton" Content="Unpatch" Margin="0,40,80,0" VerticalAlignment="Top" Height="30" Click="UnpatchButton_Click" HorizontalAlignment="Right" Width="100"/>
        <Button x:Name="DebugButton" Content="Debug" HorizontalAlignment="Right" Margin="0,119,238.333,0" VerticalAlignment="Top" Width="75" Height="30" Click="DebugButton_Click"/>

    </Grid>
</Window>
